{
  "entity": "Bancolombia",
  "account_type": "debit",
  "signature_keywords": [
    "FABIAN DARIO GRAJALES OBA",
    "91211257666",
    "FIN ESTADO DE CUENTA"
  ],
  "transaction_regex": "^(\\d{1,2}/\\d{1,2}),(.*?),(?:[^,]*,){2}\"?(-?[\\d\\.,]+)\"?,",
  "group_mapping": {
    "date": 1,
    "description": 2,
    "value": 3
  },
  "date_format": "D/M",
  "decimal_separator": ".",
  "thousand_separator": ",",
  "rules": {
    "default_negative": false,
    "positive_patterns": [
      "ABONO INTERESES",
      "PAGO DE NOMI",
      "CONSIG LOCAL",
      "DEV CUOTA MANEJO",
      "TRANSFERENCIA DESDE"
    ],
    "ignore_patterns": [
      "TRANSFERENCIA CTA SUC VIRTUAL",
      "TRASLADO A FONDO DE INVERSION"
    ]
  }
}